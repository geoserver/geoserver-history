<?xml version="1.0" encoding="UTF-8"?>
<catalog>
  <datastores>
    
    <datastore id="cdf"
               enabled="true"
               namespace="cdf">
               <!-- enabled is optional, defaults to true -->
      <description>A sample postgis testing database</description>
      <connectionParams>
        <parameter name="dbtype" value="postgis"/>
        <parameter name="host" value="localhost"/>
        <parameter name="port" value="5432"/>
        <parameter name="database" value="cite"/>
        <parameter name="user" value="cite"/>
        <parameter name="passwd" value="cite"/>
      </connectionParams>
    </datastore> 
    
    <datastore id="cgf"
               enabled="true"
               namespace="cgf">
               <!-- enabled is optional, defaults to true -->
      <description>A sample postgis testing database</description>
      <connectionParams>
        <parameter name="dbtype" value="postgis"/>
        <parameter name="host" value="localhost"/>
        <parameter name="port" value="5432"/>
        <parameter name="database" value="cite"/>
        <parameter name="user" value="cite"/>
        <parameter name="passwd" value="cite"/>
      </connectionParams>
    </datastore> 

    <datastore id="cblf--BasicPolygons" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--BasicPolygons/BasicPolygons.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--Bridges" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--Bridges/Bridges.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--BuildingCenters" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--BuildingCenters/BuildingCenters.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--Buildings" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--Buildings/Buildings.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--DividedRoutes" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--DividedRoutes/DividedRoutes.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--Forests" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--Forests/Forests.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--Lakes" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--Lakes/Lakes.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--MapNeatline" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--MapNeatline/MapNeatline.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--NamedPlaces" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--NamedPlaces/NamedPlaces.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--Ponds" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--Ponds/Ponds.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--RoadSegments" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--RoadSegments/RoadSegments.shp"/>
      </connectionParams>
    </datastore>
    <datastore id="cblf--Streams" enabled="true" namespace="cblf">
      <connectionParams>
        <parameter name="url" value="file:data/featureTypes/cblf--Streams/Streams.shp"/>
      </connectionParams>
    </datastore>
  </datastores>
  
	<!-- Defines the Coverage Formats -->
  <formats>
    <!-- a format configuration element serves as a common source -->
    <!-- repository for all coverages it holds.  -->
    <format id="arcGridSample" enabled="true">
    	<type>ArcGrid</type>
		<description>A test ArcGrid Coverage</description>
		<url>file:data/coverages/arc_sample/a.asc.gz</url>
		<parameters>
			<parameter name="Compressed" value="true"/>
			<parameter name="GRASS" value="false"/>
		</parameters>
    </format>
  </formats>
  <namespaces>
	<namespace default="true" uri="http://www.opengis.net/cite/data"
	           prefix="cdf"/>
	<namespace uri="http://www.opengis.net/cite/geometry"
	           prefix="cgf"/>
	<namespace uri="http://www.opengis.net/cite/bluelake"
	           prefix="cblf"/>
  </namespaces>

  <styles>
      <style id="dotty" filename="funkypolyshp.sld"/>
      <style id="green" filename="green.sld"/>
      <style id="maleVsFemale" filename="usa_ratio.sld"/>
      <style id="normal" filename="normal.sld" default="true"/>
  </styles>

</catalog>
