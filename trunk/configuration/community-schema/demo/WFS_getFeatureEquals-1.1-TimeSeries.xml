<wfs:GetFeature service="WFS" version="1.1.0"
  xmlns:aw="http://brs.gov.au/awdip/0.2"
  xmlns:om="http://www.opengis.net/om"
  xmlns:cv="http://www.opengis.net/cv/0.0"
  xmlns:sa="http://www.opengis.net/sampling"
  xmlns:wfs="http://www.opengis.net/wfs"
  xmlns:ogc="http://www.opengis.net/ogc"
  xmlns:gml="http://www.opengis.net/gml"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/wfs
                      http://schemas.opengis.net/wfs/1.1.0/wfs.xsd">
  <wfs:Query typeName="aw:SiteSinglePhenomTimeSeries">
    <ogc:Filter>
      <ogc:PropertyIsEqualTo>
        <ogc:PropertyName>relatedObservation/PhenomenonTimeSeries/om:result/cv:CompactDiscreteTimeCoverage/cv:element/cv:CompactTimeValuePair/cv:value</ogc:PropertyName>
        <ogc:Literal>33.2</ogc:Literal>
      </ogc:PropertyIsEqualTo>
    </ogc:Filter>
  </wfs:Query>
</wfs:GetFeature> 
